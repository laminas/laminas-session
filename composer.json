{
    "name": "laminas/laminas-session",
    "description": "manage and preserve session data, a logical complement of cookie data, across multiple page requests by the same client",
    "license": "BSD-3-Clause",
    "keywords": [
        "laminas",
        "session"
    ],
    "homepage": "https://laminas.dev",
    "support": {
        "docs": "https://docs.laminas.dev/laminas-session/",
        "issues": "https://github.com/laminas/laminas-session/issues",
        "source": "https://github.com/laminas/laminas-session",
        "rss": "https://github.com/laminas/laminas-session/releases.atom",
        "chat": "https://laminas.dev/chat",
        "forum": "https://discourse.laminas.dev"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.4-dev",
            "dev-develop": "2.5-dev"
        }
    },
    "require": {
        "php": ">=5.3.23",
        "laminas/laminas-eventmanager": "self.version",
        "laminas/laminas-stdlib": "self.version",
        "laminas/laminas-zendframework-bridge": "^1.0"
    },
    "require-dev": {
        "fabpot/php-cs-fixer": "1.7.*",
        "laminas/laminas-cache": "self.version",
        "laminas/laminas-db": "self.version",
        "laminas/laminas-http": "self.version",
        "laminas/laminas-servicemanager": "self.version",
        "laminas/laminas-validator": "self.version",
        "phpunit/phpunit": "~4.0",
        "satooshi/php-coveralls": "dev-master"
    },
    "suggest": {
        "laminas/laminas-cache": "Laminas\\Cache component",
        "laminas/laminas-db": "Laminas\\Db component",
        "laminas/laminas-eventmanager": "Laminas\\EventManager component",
        "laminas/laminas-http": "Laminas\\Http component",
        "laminas/laminas-servicemanager": "Laminas\\ServiceManager component",
        "laminas/laminas-validator": "Laminas\\Validator component"
    },
    "autoload": {
        "psr-4": {
            "Laminas\\Session\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaminasTest\\Session\\": "test/"
        }
    },
    "replace": {
        "zendframework/zend-session": "self.version"
    }
}
